set (seed:73412305)

--LOAD SYNONYMS
--read(rename:"Dendropsophus_synonym_list.txt")

--LOAD DATA
read ("dendropsophus124.ss")
read ("*.fasta")


--LOAD TAXA
--read(include:"IncludeTaxa.txt")

-- REPORT LOADED DATA
report ("v1.3_PHYG_Nakamura_crossref.csv", crossrefs, overwrite)
report ("v1.3_PHYG_Nakamura.data", data, overwrite)

--LOAD INITIAL TREES
read ("v1.1_POY_Nakamura.trees.txt")

--SEARCH SETTINGS
set (partitioncharacter:#)
set (criterion:parsimony)
set (graphtype:tree)
set (outgroup: Phyllodytes_luteolous_M)
search (hours:12, thompson:2, instances:5, exponential)

--REPORT RESULTS
report ("v1.3_PHYG_Nakamura_stats.txt", search)
report ("v1.3_PHYG_Nakamura_searchTrees.txt", graphs, newick, nohtulabels)

select (unique)
select (best)
report ("v1.3_PHYG_Nakamura_bestUnique.nwk", graphs, newick, nohtulabels)
report ("v1.3_PHYG_Nakamura_ia.tnt", tnt, concatenate, overwrite)
report ("v1.3_PHYG_Nakamura_bestConsensus.nwk", newick, reconcile, method:strict)
